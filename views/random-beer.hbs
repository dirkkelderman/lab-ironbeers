<div class="container">
  <h1>Random Beer</h1>
  <div class="row">
      {{#each randomBeer}} 
        <img src="{{this.image_url}} " alt="{{this.name}}">
        <h5>{{this.tagline}}</h4>
        <h3>{{this.name}} </h3>
        <p>{{this.description}} </p>
        <ul>
             {{#each this.food_pairing}}
                <li>{{this}}</li>
             {{/each}} 
        </ul>
        <p>{{this.brewers_tips}}</p>
      {{/each}}
  </div>
</div>