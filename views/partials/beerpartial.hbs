<div class="col-md-4">
  <div class="card">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}"/>
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <h6 class="card-title">{{this.tagline}}</h6>
            <a href="/beers/{{this.id}}">LINK</a>
            <p class="card-text">{{this.description}}</p>
        </div>
     </div>
</div>